# nia.tfvars is a templated file auto-generated by Consul NIA. The values of
# the variables are sourced from the operator nia.hcl config and values
# monitored from Consul Catalog.
#
# We should also template a warning message in the file to operators that any manual
# changes to this file will be clobbered by a subsequent update by NIA.
#

panos = {
  # Unused attributes are requried to be set explicitly to null
  hostname = "panos-example.com"
  username = "username"
  password = "password"
  api_key  = null
  protocol = null
  port     = null
  timeout  = null
}

services = {
  web : {
    # Pulled from HCL config
    name        = "web"
    description = "frontend web application"

    # Dynamic values discovered from Consul Catalog
    addresses         = [
      {
        address = "1.1.1.1"
        port = 80
      },
      {
        address = "1.1.1.2"
        port = 80
      }
    ]
  }
  api : {
    name              = "api"
    description       = "backend API for web application"
    addresses         = [
      {
        address = "2.2.2.1"
        port = 8080
      },
      {
        address = "2.2.2.2"
        port = 8080
      }
    ]
  }
}
